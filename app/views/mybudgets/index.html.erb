<div class="row">
<div class="col-md-8">
	<%= link_to 'first budget', new_mybudget_path %>

	<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Opening Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mybudget.each do |mybudget| %>
      <tr>
        <td><%= mybudget.name %></td>
        <td><%= mybudget.open_bal %></td>   >   
      </tr>
    <% end %>
  </tbody>
</table>

</div>
<div class="col-md-4">
</div>
</div>